import java.util.*;
import java.io.*;
import java.lang.*;

class gfg
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        
        while(t-- > 0)
        {
            String s = "", p = "";
            s = sc.next();
            p = sc.next();
            
            Search obj = new Search();
            
            if(obj.search(p, s) == true)
              System.out.println("Yes");
             else
              System.out.println("No");
            
        }
    }
}// } Driver Code Ends


class Search{

    /*  function to search for a pattern with distinct characters
    *   pat: pattern(to be searched in txt) given in the input
    *   txt: string given in the input
    */
    static boolean search(String s1, String s2)
    {
        int M = s1.length(); 
        int N = s2.length(); 
      
        /* A loop to slide pat[] one by one */
        for (int i = 0; i <= N - M; i++) { 
            int j; 
      
            /* For current index i, check for 
            pattern match */
            for (j = 0; j < M; j++) 
                if (s2.charAt(i + j) != s1.charAt(j)) 
                    break; 
      
            if (j == M) 
                return true; 
        } 
        return false;
    }
}
